/*
    # General Repository Information library

    Repository: @WORKSPACE_NAME@ v@@WORKSPACE_NAME@_VERSION@

    This library contains general information on the
    repository through accessible library macros.

    ***IMPORTANT***:
    (C) @OWNER@
*/

#ifndef @WORKSPACE@_INFO_HXX
#define @WORKSPACE@_INFO_HXX

// Workspace (solution/project group) name as a string
#define WORKSPACE_NAME "@WORKSPACE_NAME@"
// Workspace (solution/project group) major version
#define WORKSPACE_VERSION_MAJOR @@WORKSPACE_NAME@_VERSION_MAJOR@
// Workspace (solution/project group) minor version
#define WORKSPACE_VERSION_MINOR @@WORKSPACE_NAME@_VERSION_MINOR@
// Workspace (solution/project group) patch version
#define WORKSPACE_VERSION_PATCH @@WORKSPACE_NAME@_VERSION_PATCH@
// Workspace (solution/project group) version as a SemVer string
#define WORKSPACE_VERSION "@PROJECT_VERSION@"
// Workspace active platform architecture
#define PLATFORM_ARCHITECTURE "@CMAKE_SYSTEM_PROCESSOR@"
// Workspace active compiler
#define COMPILER "@CMAKE_CXX_COMPILER_ID@_@CMAKE_CXX_COMPILER_VERSION@"

#define @WORKSPACE@ "@WORKSPACE_NAME@ v@@WORKSPACE_NAME@_VERSION@"
#define @WORKSPACE@_VERSION_MAJOR @@WORKSPACE_NAME@_VERSION_MAJOR@
#define @WORKSPACE@_VERSION_MINOR @@WORKSPACE_NAME@_VERSION_MINOR@
#define @WORKSPACE@_VERSION_PATCH @@WORKSPACE_NAME@_VERSION_PATCH@
#define @WORKSPACE@_VERSION "@PROJECT_VERSION@"

#endif
